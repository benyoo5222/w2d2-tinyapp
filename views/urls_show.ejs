<%- include('partials/_header.ejs') %>
<body>
    <!-- Add your EJS code here -->


      <% for (var key in urls) { %> <!-- using for in loop to get the keys -->
      <% if (key === shortURL) { %> <!-- shortURL is the request id parameter that was passed to us from the user -->

      <form action =/urls/<%= shortURL%> method='POST'>
          <h1><label for="newurl"> Shortened URL: <%= shortURL %></label></h1>
          <input type= 'text' id ="newurl" name = 'newurl' style ="width:300px; height:20px" placeholder=<%= web.website %> >
          <input type='submit' value="Update">
      </form>

      <% } %>
      <% } %>


</body>
</html>